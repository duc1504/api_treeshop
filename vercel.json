{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": {"X-Requested-With": "XMLHttpRequest"}
      }
    ],
    "functions": {
      "api/*": {
        "maxDuration": 30 
      }
    }
  }
  